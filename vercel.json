{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.4",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "VERCEL": "1",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://kas-ifb-24-uas.vercel.app",
        "VITE_APP_URL": "https://kas-ifb-24-uas.vercel.app",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array"
    },
    "outputDirectory": "public"
}